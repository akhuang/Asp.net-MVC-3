@{
    ViewBag.Title = "NormalUpload";
}
<h2>
    NormalUpload</h2>
@Html.Raw(ViewBag.FilePath)
@if (ViewBag.FilePath != null)
{
    <img src="@ViewBag.FilePath" />
}
@using (Html.BeginForm("NormalUpload", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <input type="text" name="companyName" />
    <input type="file" name="companyLogo" />
    
    <input type="submit" value="submit" />
}
